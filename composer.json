{
    "name": "misfist/rr-core-functionality",
    "description": "Core functionality plugin for Rocker Radio WordPress CMS.",
    "type": "wordpress-plugin",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Pea",
            "email": "pea@misfist.com"
        }
    ],
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "johnbillion/extended-cpts",
                "version": "0.3.8",
                "type": "master",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/johnbillion/extended-cpts/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/johnbillion/extended-cpts.git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
      "johnbillion/extended-cpts": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "extra": {
        "installer-paths": {
            "./includes/lib/{$name}/": ["type:wordpress-plugin"]
        }
    }

}
